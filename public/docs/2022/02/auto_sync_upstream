          <span class="token key atrule">ref</span><span class="token punctuation">:</span> $<span class="token punctuation">{</span><span class="token punctuation">{</span> env.target_branch <span class="token punctuation">}</span><span class="token punctuation">}</span> <span class="token comment"># set the branch to merge to</span>
